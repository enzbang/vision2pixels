@@INCLUDE@@ babel.thtml
@@--
<?xml version="1.0" encoding="utf-8"?>
<response>
  @@IF@@ @_ERROR_@ = "" and @_URL_@ /= ""
    <location url="@_URL_@"/>
  @@ELSE@@
    <replace id="post_error">
    <![CDATA[
    @@IF@@ @_ERROR_@ /= ""
      @_ERROR_@
    @@ELSIF@@ @_ERROR_DUPLICATED_@ /= ""
      @_BABEL_ERROR_POST_DUPLICATED_@
    @@ELSE@@
      @_BABEL_UNKNOWN_ERROR_@
    @@END_IF@@
    ]]>
    </replace>
  @@END_IF@@
</response>
